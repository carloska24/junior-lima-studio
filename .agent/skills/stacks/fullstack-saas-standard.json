{
  "name": "fullstack-saas-standard",
  "type": "fullstack-stack",
  "description": "Complete fullstack stack for standard SaaS applications with auth, billing and admin panel.",

  "decision_profile": {
    "product_type": "saas",
    "seo": true,
    "interactivity": "high",
    "public_access": true,
    "subscription_model": true
  },

  "extends": ["nextjs-saas", "postgres-saas"],

  "frontend_stack": "nextjs-saas",
  "database_stack": "postgres-saas",

  "dependencies": ["stripe", "resend", "lucide-react"],

  "configuration": {
    "auth": "next-auth",
    "payments": "stripe",
    "email": "resend",
    "billing_model": "subscription"
  },

  "features": [
    "Authentication",
    "Subscription Management",
    "Email Notifications",
    "Admin Dashboard"
  ]
}
